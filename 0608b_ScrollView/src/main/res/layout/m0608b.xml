                android:layout_height="wrap_content"
                android:background="@color/Silver"
                android:paddingTop="5dp"
                android:text="@string/m0608b_chb02"
                android:textSize="@dimen/fontsize" />

            <CheckBox
                android:id="@+id/m0608b_chb03"
